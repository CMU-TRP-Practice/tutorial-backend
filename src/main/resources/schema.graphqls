#เอาไว้แสดง Output หรือดึงข้อมูล
type EventDTO{
    id:Int
    category:String
    title:String
    description:String
}

#เอาไว้รับ Input ถ้าไม่มี Data type ที่ต้องการ
input InputEventQuery{
    category:String
    title:String
}

input InputEvent{
    category:String
    title:String
    description:String
    organizer:InputOrganizer
}

input InputOrganizer{
    id:Int
}